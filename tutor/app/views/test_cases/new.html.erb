<%= form_for :test_case , url: test_cases_path do |f| %>
<p>
	<%= flash[:notice] %><br>
	<%= f.label :Input %>
	<%= f.text_field :input %>
	<%= f.label :Output %>
	<%= f.text_field :output  %>
</p>
<p >
	<%= f.submit 'Add' %>
</p>
<% end %>
<%= button_to "Delete Test Case", {:action => 'destroy', :id => params[:id]}, method: :delete, class:
	'btnbtn-primary', :confirm => "Are you sure you want to delete this test case ??" %>