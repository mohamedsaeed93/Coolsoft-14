<div>
	<h1>
		Search Results :
	</h1>
	<p class="lead">
		Search for Users and Courses
	</p>
</div>
<div id="results">
	<% unless @lecturers.nil? %>
		<div class="panel-group" id="accordion">
			<%= render "result_form" , objects: @lecturers %>
		</div>
	<%end%>
	<% unless @students.nil? %>
		<div class="panel-group list-group" id="accordion">
			<%= render "result_form" , objects: @students %>
		</div>
	<%end%>
	<% unless @teaching_assisstants.nil? %>
		<div class="panel-group list-group" id="accordion">
			<%= render "result_form" , objects: @teaching_assisstants %>
		</div>
	<%end%>
</div>